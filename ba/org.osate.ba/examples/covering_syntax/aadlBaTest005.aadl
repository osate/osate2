    -- a -[]-> a {x := threadGrp.int} ;
    -- a -[]-> a {x := threadGrp.intArray[0][0]} ;
    a -[]-> a {x := threadGrp.myStruct[0].int} ;
    a -[]-> a {x := threadGrp.myStruct[0].intArray[0][0]} ;
    -- a -[]-> a {x := threadGrp.recursiveGrp.dataAccessGrp.dataAccessArray[0].intArray[0][0]} ;
