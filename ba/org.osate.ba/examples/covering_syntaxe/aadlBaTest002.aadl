 -- Error tests
 
   -- Assignment action tests --
 
 a -[]-> a {int1 := int1_8} ;
 a -[]-> a {myInt1 := int1} ;
 a -[]-> a {int3 := 1.0} ;
 a -[]-> a {float3 := 1} ;
 a -[]-> a {int3 := AadlBaTestPropertySet::aFloatConstant} ;
