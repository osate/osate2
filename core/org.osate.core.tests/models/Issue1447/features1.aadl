package features1
public
	with TestPS;

	data X
		properties
			TestPS::myProp => 0;
	end X;
	
	data Y extends X
		properties
			TestPS::myProp => 10;
	end Y;

	data implementation Y.i
		properties
			TestPS::myProp => 20;
	end Y.i;

	data implementation Y.j extends Y.i
		properties
			TestPS::myProp => 30;
	end Y.j;

	system A
	end A;

	system B
	end B;

	system C
	end C;

	system D 
		features
			f1: out data port Y.j {
				TestPS::myProp => 40;
			};
	end D;
	
	system D2 extends D
		features
			f1: refined to out data port Y.j {
				TestPS::myProp => 50;
			};
	end D2;

	system implementation A.i
		subcomponents
			s1: system B.i;
		properties
			TestPS::myProp => 90 applies to s1.s2.s3.f1;
	end A.i;

	system implementation B.i
		subcomponents
			s2: system C.i;
		properties
			TestPS::myProp => 80 applies to s2.s3.f1;
	end B.i;

	system implementation C.i
		subcomponents
			s3: system D2.i;
		properties
			TestPS::myProp => 70 applies to s3.f1;
	end C.i;

	system implementation D2.i
		properties
			TestPS::myProp => 60 applies to f1;
	end D2.i;
end features1;
