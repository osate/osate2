      <import plugin="org.eclipse.ui.views.properties.tabbed"/>
      <import plugin="org.eclipse.e4.core.di"/>
      <import plugin="org.eclipse.e4.core.services"/>
      <import plugin="org.osate.ui"/>
      <import plugin="org.eclipse.zest.layouts"/>
      <import plugin="org.eclipse.e4.core.contexts"/>
      <import plugin="org.junit"/>
      <import plugin="org.eclipse.ui.navigator"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.ui.views"/>
   </requires>

   <plugin
         id="org.osate.ge"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.osate.ge.doc"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.osate.ge.diagram"
         download-size="0"
         install-size="0"
         version="1.0.0.qualifier"
         unpack="false"/>

</feature>
