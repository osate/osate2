Use of the Open Source AADL Tool Environment (OSATE) is subject
to the terms of the license set forth at http://www.aadl.info/OSATELicense.html.
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.graphiti"/>
      <import plugin="org.eclipse.graphiti.ui"/>
      <import plugin="org.eclipse.emf.transaction"/>
      <import plugin="org.eclipse.xtext.ui"/>
      <import plugin="org.osate.aadl2" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.osate.xtext.aadl2.properties" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.osate.aadl2.modelsupport" version="1.0.0" match="greaterOrEqual"/>
